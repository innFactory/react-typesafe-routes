{
  "name": "react-typesafe-routes",
  "version": "0.0.3",
  "description": "The last routing library you will ever need in your react projects - Completely typesafe and easy to use.",
  "keywords": [
    "typesafe-routing",
    "routing",
    "route-options",
    "route-middleware",
    "react-router",
    "react",
    "react-router-dom"
  ],
  "license": "MIT",
  "author": "Nicolas Schlecker (hi@nicolasschlecker.de)",
  "main": "dist/index.js",
  "module": "dist/react-typesafe-routes.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "react": "tsdx build && yarn parcel src/react/index.html",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.9.0",
    "**/@typescript-eslint/parser": "^4.9.0",
    "**/typescript": "4.1.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "@types/lodash": "^4.14.165",
    "@types/qs": "^6.9.5",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "husky": "^4.3.0",
    "lodash": "^4.17.20",
    "parcel": "^1.12.4",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "size-limit": "^4.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.1.0"
  },
  "engines": {
    "node": ">=10"
  },
  "//": {
    "husky": {
      "hooks": {
        "pre-commit": "tsdx lint"
      }
    }
  },
  "size-limit": [
    {
      "path": "dist/react-typesafe-routes.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-typesafe-routes.esm.js",
      "limit": "10 KB"
    }
  ]
}
