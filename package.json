{
  "name": "react-typesafe-routes",
  "version": "1.0.0",
  "description": "The last routing library you will ever need in your react projects - Completely typesafe and easy to use.",
  "keywords": [
    "typesafe-routing",
    "routing",
    "route-options",
    "route-middleware",
    "react-router",
    "react",
    "react-router-dom"
  ],
  "license": "MIT",
  "author": "Nicolas Schlecker (hi@nicolasschlecker.de)",
  "main": "dist/index.js",
  "module": "dist/react-typesafe-routes.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "react": "tsdx build && yarn parcel src/react/index.html",
    "doc:build": "typedoc src/index.ts",
    "doc:deploy": "yarn doc:build && gh-pages -d docs",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^5.41.0",
    "**/@typescript-eslint/parser": "^5.41.0",
    "**/typescript": "4.8.4"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "@types/qs": "^6.9.5",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "parcel": "^1.12.4",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "6.4.2",
    "size-limit": "^4.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typedoc": "^0.20.10",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-router-dom": "6.4.2",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  },
  "size-limit": [
    {
      "path": "dist/react-typesafe-routes.cjs.production.min.js",
      "limit": "15 KB"
    },
    {
      "path": "dist/react-typesafe-routes.esm.js",
      "limit": "15 KB"
    }
  ]
}
