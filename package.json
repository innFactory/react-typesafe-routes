{
  "name": "react-typesafe-routes",
  "version": "0.0.8",
  "description": "The last routing library you will ever need in your react projects - Completely typesafe and easy to use.",
  "keywords": [
    "typesafe-routing",
    "routing",
    "route-options",
    "route-middleware",
    "react-router",
    "react",
    "react-router-dom"
  ],
  "license": "MIT",
  "author": "Nicolas Schlecker (hi@nicolasschlecker.de)",
  "main": "dist/index.umd.js",
  "types": "src/index.ts",
  "files": [
    "dist",
    "src",
    "tsconfig.json",
    "vite.config.js"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsc && vite build",
    "react": "yarn run build && yarn parcel src/react/index.html",
    "doc:build": "typedoc src/index.ts",
    "doc:deploy": "yarn doc:build && gh-pages -d docs",
    "size": "size-limit",
    "postinstall": "npm run build"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "9.0.0",
    "@types/qs": "^6.9.8",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vitejs/plugin-react": "^4.1.0",
    "gh-pages": "^3.1.0",
    "parcel": "2.10.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.3.0",
    "size-limit": "^9.0.0",
    "tslib": "2.4.0",
    "typedoc": "^0.20.10",
    "typescript": "4.1.2",
    "vite": "^4.4.11"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": "6.3.0",
    "typescript": "4.1.2"
  },
  "size-limit": [
    {
      "path": "dist/react-typesafe-routes.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-typesafe-routes.esm.js",
      "limit": "10 KB"
    }
  ]
}
